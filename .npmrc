# pnpm configuration for Vercel deployment
# Fix for ERR_INVALID_THIS errors during package installation

# Fetch settings
fetch-retries=5
fetch-retry-mintimeout=20000
fetch-retry-maxtimeout=120000
fetch-timeout=180000

# Network settings
network-concurrency=1
prefer-offline=true

# Registry settings
registry=https://registry.npmjs.org/
